{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "@ui": ["components/UI"],
      "@ui/*": ["components/UI/*"],
      "@providers/*": ["components/Providers/*"],
      "@providers": ["components/Providers"],
      "@hooks/*": ["components/Hooks/*"],
      "@hooks": ["components/Hooks"],
      "@_types/*": ["types/*"]
    }
  },
  "include": [
    "process.d.ts",
    "next-env.d.ts",
    "next-auth.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next-i18next.config.js",
    "lib/sse/sse.js"
  ],
  "exclude": ["node_modules"],
  "paths": {
    "@/*": [
      "./*"
    ]
  }
}
